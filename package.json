{
  "name": "@pennockprojects/sitemap-diff",
  "version": "0.6.3",
  "description": "Finds the differences between two sitemap.xml files located on a local file system, at a web http server, or on an Amazon AWS S3 storage bucket",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PennockProjects/sitemap-diff.git"
  },
  "author": {
    "name": "John Pennock",
    "email": "info@pennockprojects.com",
    "url": "https://pennockprojects.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PennockProjects/sitemap-diff/issues"
  },
  "homepage": "https://github.com/PennockProjects/sitemap-diff#readme",
  "type": "module",
  "main": "lib/index.cjs",
  "exports": {
    "import": {
      "types": "./lib/types/index.d.ts",
      "default": "./lib/index.esm.js"
    },
    "require": {
      "types": "./lib/types/index.d.ts",
      "default": "./lib/index.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "bin": {
    "sitemap-diff": "./lib/cli.js"
  },
  "scripts": {
    "build": "rollup -c",
    "release-package": "npm run build && npx changeset publish",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "cli": "node ./lib/cli.js"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "fast-xml-parser": "^5.2.5",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@aws-sdk/lib-storage": "^3.850.0",
    "@changesets/cli": "^2.29.5",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "^3.2.4",
    "rollup": "^4.45.1",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-version-injector": "^1.3.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "sitemap",
    "sitemap.xml",
    "route path comparison",
    "sitemap diff",
    "sitemap difference",
    "sitemap comparison",
    "sitemap diff utility",
    "sitemap difference utility",
    "sitemap diff CLI",
    "sitemap difference CLI",
    "sitemap diff API",
    "sitemap difference API"
  ]
}
